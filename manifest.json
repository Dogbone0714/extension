{
    "manifest_version": 2,
  
    "name": "nHenTaiSearcher",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "0.0.1",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "拜託點我 !"
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/"
    ]
  }